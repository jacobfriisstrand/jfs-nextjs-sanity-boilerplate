    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4",
    "typescript": "^5"
