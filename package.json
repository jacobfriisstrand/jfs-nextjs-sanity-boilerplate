    "@svgr/webpack": "^8.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lint-staged": "^16.1.2",
    "next": "15.5.4",
    "next-sanity": "^10.0.7",
    "react": "^19.0.0",
    "react-dom": "^19.1.1",
    "sanity": "^4.10.3",
    "schema-dts": "^1.1.5",
    "styled-components": "^6.1.16",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.19.0",
